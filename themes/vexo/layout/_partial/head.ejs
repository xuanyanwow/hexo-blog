<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
  <meta name="description" content="<%= config.description || theme.description %>">
  <meta name="keyword" content="<%= config.keyword || theme.keyword %>">
  <% if (config.favicon || theme.favicon){ %>
    <link rel="shortcut icon" type="image/ico" href="<%= config.favicon || theme.favicon %>"/>
  <% } else { %>
    <%- favicon_tag('css/images/logo.png') %>
  <% } %>
  <title>
    <% if (page.title){ %>
      <%= page.title %> | <%= config.title %>
    <% } else { %>
      <%= config.title %>
    <% } %>
  </title>
  <link href="//cdn.bootcdn.net/ajax/libs/font-awesome/6.1.0/css/fontawesome.min.css" rel="stylesheet">
  <link href="//cdn.bootcdn.net/ajax/libs/nprogress/0.2.0/nprogress.min.css" rel="stylesheet">
  
  <%- css('css/style') %>
  <% if (theme.comment === 'gitment') { %>
    <%- css('css/plugins/gitment.css') %>
  <% } %>
  <script src="//cdn.bootcdn.net/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="//cdn.bootcdn.net/ajax/libs/geopattern/1.2.3/js/geopattern.min.js"></script>
  <script src="//cdn.bootcdn.net/ajax/libs/nprogress/0.2.0/nprogress.min.js"></script>
  <% if (theme.qrcode) { %>
    <%- js('js/qrious') %>
  <% } %>
  <% if (theme.comment === 'gitment') { %>
    <%- js('js/gitment') %>
  <% } %>
  <%- partial('_third-party/mathjax') %>
  <%- partial('_third-party/google_analytics') %>
  <%- partial('_third-party/localsearch') %>
</head>
<div class="wechat-share">
  <img src="/css/images/logo.png" />
</div>
